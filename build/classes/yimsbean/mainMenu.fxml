<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="yimsbean.MainMenuController">
   <children>
      <ImageView fitHeight="768.0" fitWidth="1024.0" layoutX="-8.0" layoutY="-8.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/BackGround.png" />
         </image>
      </ImageView>
      <Button fx:id="gonextBtn" layoutX="433.0" layoutY="422.0" mnemonicParsing="false" onAction="#gonextBtnOnAction" prefHeight="71.0" prefWidth="158.0" text="Go next" textFill="WHITE">
         <font>
            <Font name="Copperplate Gothic Bold" size="24.0" />
         </font></Button>
      <Button fx:id="creditBtn" layoutX="464.0" layoutY="508.0" mnemonicParsing="false" onAction="#creditOnAction" prefHeight="35.0" prefWidth="95.0" text="credit" textFill="WHITE">
         <font>
            <Font name="Copperplate Gothic Bold" size="18.0" />
         </font></Button>
      <Button fx:id="exitBtn" layoutX="466.0" layoutY="557.0" mnemonicParsing="false" onAction="#ExitAction" prefHeight="35.0" prefWidth="95.0" text="Exit" textFill="WHITE">
         <font>
            <Font name="Copperplate Gothic Bold" size="18.0" />
         </font></Button>
      <Slider fx:id="soundSlider" layoutX="958.0" layoutY="526.0" orientation="VERTICAL" />
      <ImageView fitHeight="39.0" fitWidth="46.0" layoutX="942.0" layoutY="697.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/sound.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
