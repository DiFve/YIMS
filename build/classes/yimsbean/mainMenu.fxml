<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="yimsbean.MainMenuController">
   <children>
      <ImageView fx:id="backGround" fitHeight="600.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/BackGround.png" />
         </image>
      </ImageView>
      <Button fx:id="gonextBtn" layoutX="371.0" layoutY="350.0" mnemonicParsing="false" onAction="#gonextBtnOnAction" text="Go next" />
      <Button layoutX="376.0" layoutY="393.0" mnemonicParsing="false" onAction="#creditOnAction" text="credit" />
      <Button layoutX="381.0" layoutY="436.0" mnemonicParsing="false" onAction="#ExitAction" text="Exit" />
   </children>
</AnchorPane>
