<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="yimsbean.MainMenuController">
   <children>
      <ImageView fx:id="backGround" fitHeight="768.0" fitWidth="1024.0" layoutX="-8.0" layoutY="-8.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/BackGround.png" />
         </image>
      </ImageView>
      <Button fx:id="gonextBtn" layoutX="483.0" layoutY="470.0" mnemonicParsing="false" onAction="#gonextBtnOnAction" text="Go next" />
      <Button layoutX="489.0" layoutY="513.0" mnemonicParsing="false" onAction="#creditOnAction" text="credit" />
      <Button layoutX="494.0" layoutY="556.0" mnemonicParsing="false" onAction="#ExitAction" text="Exit" />
   </children>
</AnchorPane>
